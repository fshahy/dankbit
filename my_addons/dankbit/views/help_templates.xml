<odoo>
<template id="dankbit_help" name="Dankbit 2x2 Image Grid">
  <t t-call="website.layout">
    <div class="container my-5">
      <div class="row text-center">
        <!-- Row 1 -->
        <div class="col-md-6 mb-4">
          <img src="/dankbit/static/src/img/LC.png"
               class="img-fluid rounded shadow-sm"
               alt="Image 1"/>
        </div>
        <div class="col-md-6 mb-4">
          <img src="/dankbit/static/src/img/LP.png"
               class="img-fluid rounded shadow-sm"
               alt="Image 2"/>
        </div>

        <!-- Row 2 -->
        <div class="col-md-6 mb-4">
          <img src="/dankbit/static/src/img/SC.png"
               class="img-fluid rounded shadow-sm"
               alt="Image 3"/>
        </div>
        <div class="col-md-6 mb-4">
          <img src="/dankbit/static/src/img/SP.png"
               class="img-fluid rounded shadow-sm"
               alt="Image 4"/>
        </div>
      </div>
    </div>
  </t>
</template>

</odoo>